{
  "ConnectionStrings": {
    "DefaultConnection": "Host=<DB_HOST>;Port=<DB_PORT>;Database=<DB_NAME>;Username=<DB_USER>;Password=<DB_PASSWORD>"
  },
  "App": {
    "CorsOrigins": "http://localhost:8080"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Stripe": {
    "PublishableKey": "pk_test_your_publishable_key_here",
    "SecretKey": "sk_test_your_secret_key_here",
    "WebhookSecret": "whsec_your_webhook_secret_here",
    "PriceIds": {
      "pro": "price_your_pro_price_id_here",
      "enterprise": "price_your_enterprise_price_id_here"
    }
  },
  "Resend": {
    "ApiKey": "re_your_resend_api_key_here",
    "FromEmail": "noreply@yourdomain.com",
    "FromName": "ShipMvp",
    "BaseUrl": "https://api.resend.com",
    "TimeoutSeconds": 30,
    "MaxRetryAttempts": 3
  },
  "GoogleAnalytics": {
    "PropertyId": "your_ga4_property_id_here",
    "CredentialsPath": "",
    "ServiceAccountEmail": "",
    "PrivateKey": "",
    "ApplicationName": "ShipMvp",
    "EnableCaching": true,
    "CacheDurationMinutes": 15,
    "TimeoutSeconds": 30
  },
  "AllowedHosts": "*",
  "Integrations": {
    "Google": {
      "ClientId": "<GOOGLE_CLIENT_ID>",
      "ClientSecret": "<GOOGLE_CLIENT_SECRET>",
      "RedirectUri": "http://localhost:4200/integration-platforms"
    }
  }
 
}
